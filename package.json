{
    "name": "fca-asuna-remake",
    "version": "1.0.1",
    "description": "Facebook Chat Api Được Remake Bới NHQ",
    "main": "index.js",
    "scripts": {
        "test": "mocha",
        "lint": "./node_modules/.bin/eslint **.js",
        "prettier": "prettier utils.js src/* --write"
    },
    "author": "Avery, David, Maude, Benjamin, KanzuWakazaki, Asuna",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/asuna2003/fca-asuna-remake/issues"
    },
    "homepage": "https://github.com/asuna2003/fca-asuna-remake#readme",
    "repository": {
        "type": "git",
        "url": "git@github.com:asuna2003/fca-asuna-remake.git"
    },
    "dependencies": {
        "aes-js": "latest",
        "assert": "latest",
        "bluebird": "latest",
        "chalk": "4.1.2",
        "cheerio": "latest",
        "crypto": "latest",
        "crypto-js": "latest",
        "got": "^11.8.3",
        "horizon-sp": "latest",
        "https-proxy-agent": "latest",
        "is-hexcolor": "^1.0.0",
        "lodash": "",
        "mqtt": "latest",
        "node-superfetch": "",
        "npmlog": "latest",
        "os": "latest",
        "path": "latest",
        "pretty-ms": "7.0.1",
        "request": "latest",
        "websocket-stream": "latest",
        "uuid-apikey": "latest",
        "express": "latest",
        "readline": "latest",
        "totp-generator": "latest",
        "synthetic-horizon-database": "latest"
    },
    "engines": {
        "node": ">=14.x <16.x"
    },
    "devDependencies": {
        "eslint": "latest",
        "mocha": "latest",
        "prettier": "^2.7.1"
    },
    "eslintConfig": {
        "env": {
            "es6": true,
            "node": true
        },
        "extends": "eslint:recommended",
        "parserOptions": {
            "sourceType": "module"
        },
        "rules": {
            "linebreak-style": [
                "error",
                "unix"
            ],
            "semi": [
                "error",
                "always"
            ],
            "no-unused-vars": [
                1,
                {
                    "argsIgnorePattern": "^_"
                }
            ]
        }
    },
    "keywords": [
        "Fca horizon",
        "fca-horizon-remake",
        "horizon fca",
        "horizon"
    ]
}
